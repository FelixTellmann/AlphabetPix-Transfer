  color: $color-footer-text;
  padding: ($gutter * 2) 0;

  .site-footer--hidden & {
    display: none;
  }

  .supports-no-js & {
    display: block;
  }

  a {
    color: $color-footer-text;

    &:hover,
    &:focus {
      opacity: $link-hover-opacity;
    }
  }

  .input-group--underline {
