/*================ AJAX Cart Footer ================*/

.cart-drawer__footer {
  min-height: 230px;
  padding: 0 $gutter;
  border-top: 1px solid darken($color-secondary-bg, 5%);
  background-color: $color-secondary-bg;
  text-align: center;
  opacity: 1;
  transition: opacity 0.25s ease-out, min-height 0.25s ease-out;

  .cart-drawer--empty & {
    opacity: 0;
    pointer-events: none;
    min-height: 0;
    max-height: 0;
  }

  @include media-query($medium-up) {
    padding: 0 ($gutter * 2);
  }
}

.cart-drawer__footer-container {
  padding: $gutter 0;

  @include media-query($medium-up) {
    padding: ($gutter * 2) 0;
  }
}

.cart-drawer__message-container {
  opacity: 0;

  .ajax-cart--error & {
    opacity: 1;
  }
}

.cart-drawer__message {
  text-align: center;
  font-size: 14px;
  color: $color-error;

  .ajax-cart--error & {
    padding-bottom: 40px;
  }
}

.cart-drawer__discount-container {
  max-height: 0;
  opacity: 0;
  overflow: hidden;
  transition: opacity 0.25s ease-out;
}

.cart-drawer__discount {
  max-height: 0;
  display: table;
  width: 100%;
  margin-bottom: $gutter / 2;
  font-size: rem(14);
  overflow: hidden;
  transition: max-height 0.25s ease-in-out;
}

.cart-drawer__subtotal {
  display: table;
  width: 100%;
  margin-bottom: $gutter / 2;
}


.cart-drawer__subtotal-label {
