.product-slideshow__content {
  height: 100%;
  padding: ($gutter * 4) 0 ($gutter * 7);
}

.product-slideshow__slide {
  .product-slideshow--single & {
    cursor: default;
  }
}

.product-slideshow__slide-image-wrapper {
  display: none;
  position: relative;
  width: 100%;
  height: 100%;

  .product-slideshow--show & {
    display: block;
  }
}

.product-slideshow__slide-image {
  width: 100%;
  height: 100%;
  object-fit: contain;
  font-family: "object-fit: contain";
  pointer-events: none;

  &.lazyloaded + .sk-fading-circle {
    display: none;
  }
}

.product-slideshow__controls {
  text-align: center;
  padding: ($gutter * 2) 0;
}

.product-slideshow__slide-select-list {
  display: inline-block;
  padding: 0 $gutter;
}

.product-slideshow__slide-select {
  @include prefix(appearance, none, webkit moz spec);
  background-color: $color-body-text;
  border-radius: 6px;
  border-width: 0;
  margin: 0 4px;
  padding: 0;
  height: 4px;
  width: 4px;
  opacity: 0.7;
  transition: opacity 0.15s ease-out;
}

.product-slideshow__slide-select--active {
  background-color: $color-primary;
  opacity: 1;
}

.product-slideshow__next,
.product-slideshow__previous {
  .icon {
    width: 6px;
    margin-top: -4px;
    fill: $color-primary;
  }
}


.quotes-slider {
  height: 500px;
  background-color: $settings-color-main-bg;
  position: relative;
  overflow: hidden;

  @include media-query($medium-up) {
    height: 400px;
  }
}

.quotes-slide {
  padding: 0 $gutter;
  position: absolute;
  width: 100%;
  max-width: 85%;
  top: 50%;
  left: 50%;
  opacity: 0;
  z-index: 0;
  pointer-events: none;
  @include transform(translate(-50%, -50%));
  transition: opacity 0.5s ease-out;
}

.quotes-slide--visible {
  opacity: 1;
  z-index: 1;
}

.quote {
  margin-bottom: $gutter;
}

.quote-author {
  letter-spacing: 1px;
}

.search-again-button,
.navigation__search {
  margin: ($gutter * 2) auto 0;

  @include media-query($medium-up) {
    margin: ($gutter * 3.5) auto 0;
  }
}

.supports-no-js .search-again-button {
  display: none;
}

.supports-no-js .navigation__search {
  position: relative !important; // sass-lint:disable-line no-important
  overflow: initial;
}

/*================ Section header ================*/
.section-header-image-wrapper {
  height: 278px;
  overflow: hidden;
  background-color: $color-secondary-bg;

  @include media-query($medium-up) {
    height: 390px;
  }

  @include media-query($widescreen) {
    height: 560px;
  }
}

.section-header-image {
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  height: 100%;
}

.section-header-image--parallax {
  @include media-query($medium-up) {
    background-attachment: fixed;
  }

  .is-mobile-os & {
    background-attachment: scroll;
  }
}

.section-header {
  margin: ($gutter * 2.5) 0;

  @include media-query($medium-up) {
    margin: ($gutter * 3) 0;
  }
}

.section-header--small {
  margin: ($gutter * 2) 0;

  @include media-query($medium-up) {
    margin: ($gutter * 2.5) 0;
  }
}

.section-header__title {
  margin-bottom: 0;
}

.section-header__description,
.section-header__btn {
  margin-top: $gutter;
}

.section-header__subtext {
  display: block;

  @include media-query($medium-up) {
    margin: (-$gutter / 2) 0 ($gutter * 3.5);
  }
}

$z-index-slideshow-image: 1;
$z-index-slideshow-video: 2;
$z-index-slideshow-text: 3;
$z-index-slideshow-buttons: 4;
$z-index-slideshow-dots: 5;
$z-index-slideshow-mobile-buttons: 6;
$transition-buttons: all 0.35s cubic-bezier(0, 0, 0, 1);
$transition-slideshow: all 0.5s cubic-bezier(0, 0, 0, 1);
$transition-image-slideshow: opacity 0.8s cubic-bezier(0.44, 0.13, 0.48, 0.87), filter 400ms, -webkit-filter 400ms;

.slideshow {
  @include display-flexbox;
  position: relative;
  overflow: hidden;
  height: calc(100vh - 35px);
  min-height: 500px;
}

.slideshow__overlay {
  @include overlay(3);
}

.slideshow__slide {
  position: absolute;
  height: 100%;
  width: 100%;
  opacity: 0;
  z-index: 0;
  transition: $transition-slideshow;
}

.slideshow__slide--active {
  transition: $transition-slideshow;
  opacity: 1;
  z-index: 2;
}

.slideshow__slide--transitioning {
  opacity: 1;
  z-index: 1;
}

.slideshow__image {
  height: 100%;
  width: 100%;
  object-fit: cover;
  z-index: $z-index-slideshow-image;
  transition: $transition-image-slideshow;
  font-family: "object-fit: cover";

  .placeholder-background {
    background-color: $color-main-bg;
  }

  .placeholder-svg {
    position: absolute;
    top: 50%;
    left: 50%;
    width: auto;
    max-height: none;
    max-width: none;
    @include transform(translate(-50%, -50%));
  }
}

.slideshow__video,
.slideshow__video iframe {
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 101%;
  visibility: hidden;
  transition: $transition-slideshow;
  pointer-events: none;

  @include media-query($small) {
    display: none;
  }

  // https://developer.mozilla.org/en-US/docs/Web/CSS/Media_Queries/Using_media_queries#aspect-ratio
  @media (min-aspect-ratio: 16/9) {
    height: 300%;
    top: -100%;
  }

  @media (max-aspect-ratio: 16/9) {
    width: 300%;
    left: -100%;
  }

  .slideshow__video--loaded & {
    opacity: 1;
    visibility: visible;
  }
}

.slideshow__video::after {
  content: '';
  display: block;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: $z-index-slideshow-video;
}

.slideshow__text-container {
  text-align: center;
  position: absolute;
  top: 50%;
  left: 0;
  @include transform(translate3d(0, -50%, 0));
  z-index: $z-index-slideshow-text;
  width: 100%;
  max-width: 100%;

  .ie9 & {
    @include transform(translate(0, -50%));
  }
}

.slideshow__text-content {
  opacity: 0;

  &.has-animated {
    @include prefix(animation, fadeUp 0.5s ease-out 0.5s 1 forwards, webkit spec);
  }

  .ie9 &,
  .supports-no-js &,
  .animations--disabled & {
    opacity: 1;
  }
}

.slideshow__heading {
  margin: 0 auto;

  @include media-query($medium-up) {
    max-width: 800px;
  }
}

.slideshow__heading-cta {
  margin-top: $gutter * 2;

  @include media-query($medium-up) {
    margin-top: $gutter * 3;
  }
}

.slideshow__buttons {
  @include display-flexbox;
  align-items: flex-end;
  position: absolute;
  bottom: 0;
  z-index: $z-index-slideshow-buttons;
  transition: $transition-buttons;
  overflow: visible;

  .supports-no-js & {
    display: none;
  }
}

.slideshow__button {
  @include display-flexbox;
  @include flex-direction(column);
  @include align-items(center);
  @include flex(0 0 50%);
  position: relative;
  text-align: center;
  white-space: initial;
  transition: $transition-buttons;

  @include media-query($small) {
    @include transform(translateX(50%));
    margin-bottom: $gutter;
  }

  @include media-query($medium-up) {
    @include flex(1 1 100%);

    &:hover:not(.slideshow__button--active) {
      @include transform(translate3d(0, -16px, 0));
    }
  }

  &:not(.slideshow__button--active) {
    @include align-self(flex-end);
  }
}

.slideshow__button--4,
.slideshow__button--5 {
  @include media-query($medium-up) {
    @include transform(translateX(-50%));

    &.slideshow__button--active {
      @include transform(translate3d(-50%, -($gutter * 3), 0));
    }

    &:hover:not(.slideshow__button--active) {
      @include transform(translate3d(-50%, -16px, 0));
    }
  }
}

.slideshow__button--4 {
  @include media-query($medium-up) {
    @include flex(1 0 33%);
  }
}

.slideshow__button--5 {
  @include media-query($medium-up) {
    @include flex(1 0 25%);
  }
}

.slideshow__button--active {
  @include transform(translate3d(50%, -($gutter * 3), 0));

  &:not(.slideshow__button--link) {
    @include transform(translate3d(50%, -16px, 0));

    @include media-query($medium-up) {
      @include transform(translate3d(0, -16px, 0));

      &.slideshow__button--4,
      &.slideshow__button--5 {
        @include transform(translate3d(-50%, -16px, 0));
      }
    }
  }

  @include media-query($medium-up) {
    @include transform(translate3d(0, -($gutter * 3), 0));
  }
}

.slideshow__button--previous,
.slideshow__button--next {
  position: absolute;
  height: 100%;
  width: 10vh;
  background: transparent;
  border: 0;
  z-index: $z-index-slideshow-mobile-buttons;

  @include media-query($medium-up) {
    visibility: hidden;
    z-index: $z-index-slideshow-buttons;
  }
}

.slideshow__button--next {
  right: 0;
}

.slideshow__button-label {
  color: $color-heading-text;

  @include media-query($medium-up) {
    padding: $gutter ($gutter * 2);
    margin: 4px 0 35px 0;
  }
}

.slideshow__button,
.slideshow__button-label {
  .ie9 & {
    display: none;
  }
}

.slideshow__button-cta {
  position: absolute;
  left: 50%;
  bottom: -62px;
  @include transform(translateX(-50%));

  @include media-query($medium-up) {
    bottom: (-$gutter) * 3;
  }

  .slideshow__button:not(.slideshow__button--active) & {
    pointer-events: none;
  }

  .slideshow__button--active.slideshow__button--link & {
    bottom: -50px;
    @include media-query($medium-up) {
      bottom: (-$gutter);
    }
  }

  .slideshow__button--active:not(.slideshow__button--link) & {
    bottom: (-$gutter) * 4;

    @include media-query($medium-up) {
      bottom: (-$gutter) * 3;
    }
  }
}

.slideshow__button-cta--mobile {
  padding: 0;
  height: 40px;
  width: 40px;
  line-height: 40px;
  border: 0;

  .icon-plus {
    height: 8px;
    width: 8px;
    fill: $color-button-text;
  }
}

.slideshow__nav {
  @include display-flexbox;
  @include align-items(center);
  @include justify-content(center);
  z-index: $z-index-slideshow-dots;

  .supports-no-js & {
    display: none;
  }

  @include media-query($small) {
    height: 35px;
  }

  @include media-query($medium-up) {
    display: none;
  }
}

.slideshow__nav-dot {
  background-color: $color-body-text;
  border-radius: 6px;
  border-width: 0;
  margin: 0 5px;
  padding: 0;
  height: 4px;
  width: 4px;
  opacity: 0.7;
  transition: opacity 0.15s ease-out;
}

.slideshow__nav-dot--active {
  background-color: $color-primary;
  opacity: 1;
}

@include keyframes(fadeUp) {
  0% {
    @include transform(translate3d(0, 10%, 0));
    opacity: 0;
  }

  100% {
    @include transform(none);
    opacity: 1;
  }
}

$site-header-wrapper-color: rgba($color-header, 0);

/*================ Site Header ================*/

.site-header {
  position: relative;
  transition: transform 0.25s ease-out;
  z-index: $z-index-header;
}

.site-header--transparent,
.site-header--homepage {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: $z-index-header;

  html.supports-no-js & {
    position: relative;
  }
}

.site-header--fixed {
  position: fixed;
  top: 0;
  width: 100%;
  @include prefix(animation, slideDown 0.25s ease-out, webkit spec);
}

.site-header__wrapper {
  position: relative;
  width: 100%;
  background-color: $color-header;
  z-index: $z-index-header;
  transition: background-color 0.25s ease-out;

  .site-header--transparent & {
    background-color: $site-header-wrapper-color;
  }

  .navigation-open &,
  html.supports-no-js & {
    background-color: $color-header;
  }

  .site-header--fixed & {
    background-color: $color-header;
  }

  svg {
    fill: $color-header-links;
  }
}

.site-header__wrapper--transitioning {
  transition: background-color 0.25s ease-out, margin-top 0.25s ease-out;
}


.site-header-sections {
  @include display-flexbox;
  @include align-items(center);

  .ie9 & {
    display: table;
    width: 100%;
  }
}

.site-header__section {
  .ie9 & {
    display: table-cell;
    vertical-align: middle;
  }
}

.site-header__section--button {
  .ie9 & {
    width: 150px;
    white-space: nowrap;
  }
}

.site-header__section--title {
  @include display-flexbox;
  @include justify-content(center);

  .ie9 & {
    width: 100%;
    padding: ($gutter / 2) 0 2px;
    text-align: center;
  }
}

.site-header__logo-wrapper {
  display: inline-block;
  margin-bottom: 0;
  padding: ($gutter / 2) 0;
  text-align: center;
  line-height: 1.2;
}

.site-header__logo {
  display: block;
}

.site-header__logo-image {
  display: block;
  margin: 0 auto;

  .site-header--transparent & {
    display: none;
  }

  .navigation-open &,
  html.supports-no-js & {
    display: block;
  }

  .site-header--fixed & {
    display: block;
  }
}

.site-header__logo-image--transparent {
  display: none;

  .site-header--transparent & {
    display: block;
  }

  .navigation-open &,
  html.supports-no-js & {
    display: none;
  }

  .site-header--fixed & {
    display: none;
  }
}

.site-header__cart {
  svg {
    width: 22px;
    height: 30px;
    margin: -2px 0 0 -2px;

    @include media-query($medium-up) {
      width: 26px;
    }
  }

  &::after {
    border-color: $color-header-links;
  }
}

.site-header__cart-bubble {
  position: absolute;
  top: 15px;
  right: 10px;
  width: 10px;
  height: 10px;
  background-color: $color-cart-flag;
  border-radius: 50%;
  opacity: 0;
  transition: all 0.25s ease-out;
  @include transform(scale(0));

  @include media-query($medium-up) {
    top: 15px;
    right: 9px;
  }

  .site-header__cart & {
    top: 13px;

    @include media-query($medium-up) {
      top: 12px;
    }
  }
}

.site-header__cart-bubble--visible {
  opacity: 1;
  @include transform(scale(1));

  &.bubble-pulse {
    @include prefix(animation, bubble-pulse 0.5s ease-out, webkit spec);
  }
}

@include keyframes(bubble-pulse) {
  0% {
    @include transform(scale(1));
  }

  25% {
    @include transform(scale(1.4));
  }

  50% {
    @include transform(scale(1));
  }

  75% {
    @include transform(scale(1.4));
  }

  100% {
    @include transform(scale(1));
  }
}

.burger-icon {
  display: block;
  height: 2px;
  width: 18px;
  left: 50%;
  margin-left: -9px;
  background: $color-header-links;
  position: absolute;
  transition: all 0.25s ease-out;

  .navigation-open & {
    width: 24px;
    top: calc(50% - 1px);
    left: calc(50% - 4px);
  }

  @include media-query($medium-up) {
    height: 2px;
    width: 24px;
    left: 50%;
    margin-left: -12px;

    .navigation-open & {
      width: 30px;
      top: calc(50% - 1px);
      left: calc(50% - 2px);
    }
  }
}

.burger-icon--top {
  top: 17px;

  .navigation-open & {
    @include transform(rotate(45deg));
  }

  @include media-query($medium-up) {
    top: 16px;

    .site-header__navigation:hover & {
      @include transform(translateY(-2px));

      .navigation-open & {
        width: 35px;
        left: calc(50% - 4px);
        @include transform(rotate(45deg));
      }
    }
  }
}

.burger-icon--mid {
  top: 24px;

  @include media-query($medium-up) {
    top: 24px;
  }

  .navigation-open & {
    opacity: 0;
  }
}

.burger-icon--bottom {
  top: 31px;

  .navigation-open & {
    @include transform(rotate(-45deg));
  }

  @include media-query($medium-up) {
    top: 32px;

    .site-header__navigation:hover & {
      @include transform(translateY(2px));

      .navigation-open & {
        width: 35px;
        left: calc(50% - 4px);
        @include transform(rotate(-45deg));
      }
    }
  }
}

.site-header__navigation {
  position: relative;
  transition: transform 0.15s ease-out;

  &:hover {
    opacity: 1;
  }

  svg {
    width: 20px;
    height: 30px;
  }

  &::after {
    border-color: $color-header-links;
  }
}

.social-links {
  .icon {
    fill: $color-body-text;
  }

  a {
    margin-right: $gutter;
    text-decoration: none;
    opacity: 1;
    transition: opacity 0.15s ease-out;
  }

  a:hover,
  a:focus {
    opacity: $link-hover-opacity;
  }

  a:last-child {
    margin-right: 0;
  }
}

.social-links--large {
  .icon {
    width: 24px;
    height: 24px;
    margin-top: 6px;
  }
}

.social-links__icon {
  margin-right: $gutter / 2;

  &:last-child {
    margin-right: 0;
  }
}

$social-sharing-link-border-color: rgba($color-body-text, 0.1);

/*================ Social Sharing ================*/

.social-sharing {
  position: relative;
  display: inline-block;
  margin: 0 auto;
}

.social-sharing--product {
  margin-bottom: 80px;
}

.social-sharing--article {
  &.social-sharing--1 {
    height: 145px;
  }

  &.social-sharing--2 {
    height: 215px;
  }

  &.social-sharing--3 {
    height: 285px;
  }
}

.social-sharing__toggle {
  padding: $gutter;
  z-index: 1;
  cursor: pointer;

  .social-sharing--article & {
    padding: 0;
  }
}

.social-sharing__item-list {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 0;
  padding: 0;
  margin: 0;
  list-style: none;

  .social-sharing--mobile & {
    box-sizing: content-box;
    max-height: 50px;
  }
}

.social-sharing__item {
  display: inline-block;
  position: absolute;
  top: 0;
  left: 50%;
  width: 50px;
  height: 50px;
  margin: 0 0 0 -25px;
  opacity: 0;
  pointer-events: none;
  transition: all 0.15s ease-out;

  .social-sharing--active & {
    opacity: 1;
    pointer-events: auto;
  }
}


.social-sharing__link {
  display: inline-block;
  width: 50px;
  height: 50px;
  line-height: 43px;
  border-radius: 50%;
  text-align: center;
  border: solid 1px rgba(0, 0, 0, 0.35);
  transition: all 0.15s ease-out;
  background-color: $color-main-bg;
  
  @include max-size(sm) {
    width: 40px;
    height: 40px;
    line-height: 34px;
  }

  &:hover {
    border: solid 1px rgba(0, 0, 0, 0.6);
    background-color: rgba(255,255,255, 0.8);
    .icon {
      fill: $color-body-text;
    }
  }

  .social-sharing--active & {
    opacity: 1;
    pointer-events: auto;
  }

  .icon {
    fill: $color-body-text;
  }
}

.social-sharing__icon {
  .social-sharing--product & {
    margin-right: 10px;
  }

  .social-sharing--mobile & {
    display: block;
    width: 50px;
    height: 50px;
    padding: 8px 0;
    margin: 0 auto ($gutter / 2);
    text-align: center;
    border: 2px solid $color-primary;
    border-radius: 50px;
  }

  .social-sharing--article & {
    display: block;
    width: 100%;
    height: 15px;
    padding: 0;
    margin-bottom: 15px;
    text-align: center;

    .icon {
      vertical-align: top;
    }
  }

  .icon {
    fill: $color-primary;
    height: 15px;
  }

  .icon-share {
    display: inline-block;
    transition: all 0.15s ease-out;
    @include transform(rotateX(0deg));

    .social-sharing--active & {
      opacity: 0;
      @include transform(rotateX(180deg));
    }
  }

  .icon-close {
    display: inline-block;
    margin-left: -25px;
    opacity: 0;
    transition: all 0.15s ease-out;
    @include transform(rotateX(0deg));

    .social-sharing--active & {
      opacity: 1;
      @include transform(rotateX(180deg));
    }
  }

  .icon-facebook {
    height: 22px;
  }
}

.social-sharing__item:nth-of-type(1) {
  .social-sharing--1.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(0, -60px));
  }

  .social-sharing--1.social-sharing--product.social-sharing--active & {
    @include transform(translate(0, 65px));
  }

  .social-sharing--2.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(-30px, -60px));
  }

  .social-sharing--2.social-sharing--product.social-sharing--active & {
    @include transform(translate(-30px, 65px));
  }

  .social-sharing--3.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(-60px, -45px));
  }

  .social-sharing--3.social-sharing--product.social-sharing--active & {
    @include transform(translate(-65px, 65px));
  }

  .social-sharing--article.social-sharing--active & {
    @include transform(translate(0, 105px));
  }
}

.social-sharing__item:nth-of-type(2) {
  transition-delay: 0.05s;
  -webkit-transition-delay: 0.05s;

  .social-sharing--active & {
    opacity: 1;
  }

  .social-sharing--2.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(30px, -60px));
  }

  .social-sharing--2.social-sharing--product.social-sharing--active & {
    @include transform(translate(30px, 65px));
  }

  .social-sharing--3.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(0, -60px));
  }

  .social-sharing--3.social-sharing--product.social-sharing--active & {
    @include transform(translate(0, 65px));
  }

  .social-sharing--article.social-sharing--active & {
    @include transform(translate(0, 165px));
  }
}

.social-sharing__item:nth-of-type(3) {
  transition-delay: 0.1s;
  -webkit-transition-delay: 0.1s;

  .social-sharing--active & {
    opacity: 1;
  }

  .social-sharing--3.social-sharing--mobile.social-sharing--active & {
    @include transform(translate(60px, -45px));
  }

  .social-sharing--3.social-sharing--product.social-sharing--active & {
    @include transform(translate(65px, 65px));
  }

  .social-sharing--article.social-sharing--active & {
    @include transform(translate(0, 225px));
  }
}







/*================ Slider ================*/
.slider {
  position: relative;
  width: 100%;
  height: 100%;
}

.slider__slide {
  display: none;
  position: absolute;
  width: 100%;
  height: 100%;
  cursor: pointer;
  cursor: -webkit-grab;
  cursor: -moz-grab;
}

.slider__slide--active {
  display: block;
  @include transform(translateX(0));
}

.slider__slide--previous {
  display: block;
  @include transform(translateX(-100%));

}

.slider__slide--next {
  display: block;
  @include transform(translateX(100%));
}

.slider__slide--transitioning {
  transition: transform 0.2s ease-out;
}

.slider__slide--dragging {
  cursor: -webkit-grabbing;
  cursor: -moz-grabbing;
}

.sk-fading-circle {
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -20px 0 0 -10px;
  width: 40px;
  height: 40px;
}

.supports-no-js .sk-fading-circle {
  display: none;
}

.sk-fading-circle--top {
  margin-top: $gutter * 2.5;
}

.sk-circle {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}

.sk-circle::before {
  content: '';
  display: block;
  margin: 0 auto;
  width: 15%;
  height: 15%;
  background-color: $color-primary;
  border-radius: 100%;
  @include prefix(animation, sk-circleFadeDelay 1.2s infinite ease-in-out both, webkit spec);
}

@for $i from 1 through 11 {
  $item: $i + 1;
  $deg: ($i * 30) + deg;
  $time: -(1.2 - ($i * 0.1)) + s;

  .sk-circle#{$item}::before {
    @include prefix(animation-delay, $time, webkit spec);
  }

  .sk-circle#{$item} {
    @include prefix(transform, rotate($deg), webkit ms spec);
  }
}

@include keyframes(sk-circleFadeDelay) {
  0%,
  39%,
  100% {
    opacity: 0;
  }

  40% {
    opacity: 1;
  }
}

@include keyframes(sk-circleFadeDelay) {
  0%,
  39%,
  100% {
    opacity: 0;
  }

  40% {
    opacity: 1;
  }
}

$video-section-primary-color: #fff;
$video-section-icon-color: #4a4a4a;
$video-section-overlay-background-color: rgba(0, 0, 0, 0.8);
$video-section-button-background-color: rgba(0, 0, 0, 0);

.video-section {
