/*================ Navigation ================*/

.navigation {
  position: absolute;
  top: 0;
  left: 0;
  background-color: $color-main-bg;
  max-height: 0;
  width: 100%;
  text-align: center;
  overflow-x: hidden;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch; // sass-lint:disable-line no-misspelled-properties
  opacity: 0;
  transition: opacity 0.25s ease-out;
  pointer-events: none;
  z-index: 3;

  html.supports-no-js & {
    max-height: none;
    position: relative;
    opacity: 1;
    pointer-events: auto;
  }
}

.navigation__container {
  width: 100%;
  min-height: 100%;
  padding-top: ($gutter * 3);
  padding-bottom: ($gutter * 3);
}

.navigation-open {

  .navigation {
    max-height: none;
    height: 100vh;
    opacity: 1;
    pointer-events: auto;
  }

  .content-for-layout {
    display: none;
  }

  .site-footer {
    display: none;
  }
}

.navigation--is-transitioning {
  .navigation {
    position: absolute;
    max-height: none;
    height: 100vh;
    width: 100%;
  }

  .content-for-layout {
    display: block !important; // sass-lint:disable-line no-important
  }

  .site-footer {
    display: block !important; // sass-lint:disable-line no-important
  }
}

.navigation__entrance-animation {
  max-height: 1000px;
  opacity: 0;
  @include transform(translateY(20px));
  transition: all, transform 0.25s ease-out, opacity 0.25s ease-out;

  &.navigation__entrance-animation--active,
  .navigation-open--editor & {
    max-height: 1001px;
    opacity: 1;
    @include transform(translateY(0));
  }

  html.supports-no-js & {
    opacity: 1;
  }
}

/*================ Navigation Links ================*/

.navigation__links {
  list-style: none;
  padding-left: 0;
  margin: 0 0 $gutter;

  @include media-query($medium-up) {
    margin-bottom: $gutter * 3;
  }
}

.navigation__link {
  a {
    display: inline-block;
    position: relative;
    font-size: rem(14);
    line-height: line-height(64, 18);
    font-weight: $font-weight-bold;
    letter-spacing: rem(2.5);
    color: $color-heading-text;
    text-decoration: none;
    text-transform: uppercase;
    opacity: 1;

    @include media-query($medium-up) {
      font-size: rem(20);
      line-height: line-height(48, 20);
      font-weight: $font-weight-bold;
    }
  }
}

.navigation__link > a,
.navigation__link.navigation__has-sublinks--collapsed > button {
  transition: transform 0.25s ease-out;

  &:after {
    content: '';
    display: block;
    position: relative;
    bottom: rem(9);
    width: 0;
    margin: 0 auto;
    height: 2px;
    background-color: $color-body-text;
    transition: all 0.25s;
  }

  @include media-query($medium-up) {
    &:hover {
      @include transform(translateY(-5px));

      &:after {
        @include transform(translateY(5px));
        width: 100%;
      }
    }

    &:hover:active {
      @include transform(translateY(0));

      &:after {
        @include transform(translateY(0));
      }
    }
  }
}

.navigation__expand-sublinks {
  @include prefix('user-select', 'none', moz ms webkit spec);
  @include prefix('appearance', none, webkit moz spec);
  position: relative;
  padding: 0;
  font-size: rem(14);
  line-height: line-height(64, 18);
  font-weight: $font-weight-bold;
  letter-spacing: rem(2.5);
  color: $color-heading-text;
  background-color: transparent;
  border: 0;
  text-decoration: none;
  text-transform: uppercase;
  opacity: 1;
  transition: all 0.25s ease-out;

  &:after {
    content: '';
    display: block;
    position: relative;
    bottom: rem(9);
    width: 0;
    margin: 0 auto;
    height: 2px;
    background-color: $color-body-text;
    transition: all 0.25s;
  }

  @include media-query($medium-up) {
    font-size: rem(20);
    line-height: line-height(48, 20);
    font-weight: $font-weight-bold;

    &:hover {
      @include transform(translateY(-5px));

      &:after {
        @include transform(translateY(5px));
      }
    }

    &:hover:active {
      @include transform(translateY(0));

      &:after {
        @include transform(translateY(0));
      }
    }
  }

  svg {
    width: 10px;
    position: absolute;
    right: -$gutter;
    top: 50%;
    margin-top: -10px;
    fill: $color-primary;
    @include transform(rotateZ(180deg));
    transition: transform 0.25s ease-out;

    .ie9 & {
      top: 10%;
    }

    .navigation__has-sublinks--collapsed & {
      @include transform(rotateX(0deg));
    }
  }
}

.navigation__sublinks-container {
  transition: all 0.15s ease-out;
  background-color: $color-secondary-bg;
  overflow: hidden;
  visibility: visible;

  .navigation__has-sublinks--collapsed & {
    max-height: 0 !important; // sass-lint:disable-line no-important
    visibility: hidden;
  }

  html.supports-no-js & {
    max-height: none !important; // sass-lint:disable-line no-important
    visibility: visible;
  }
}

.navigation__sublinks {
  overflow: hidden;
  list-style: none;
  padding: $gutter 0;
}

.navigation__sublink {
  a {
    font-size: rem(14);
    font-weight: $font-weight-normal;
    line-height: line-height(42, 14);
    text-transform: none;
    transition: opacity 0.25s ease-out;

    @include media-query($medium-up) {
      font-size: rem(16);
      line-height: line-height(42, 16);

      &:hover {
        opacity: $link-hover-opacity;
      }
    }
  }
}

.navigation__link--secondary {
  padding: 0;

  a {
    font-size: rem(14);
    line-height: line-height(48, 14);
    font-weight: $font-weight-normal;
    text-transform: none;
    letter-spacing: rem(2);
    transition: opacity 0.25s ease-out;

    @include media-query($medium-up) {
      line-height: line-height(48, 20);

      &:hover {
        opacity: $link-hover-opacity;
        @include transform(translate(0, 0));
      }
    }

    &:after {
      content: none;
    }
  }
}

