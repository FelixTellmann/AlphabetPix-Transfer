/*-------------------------------------*
  #Base HTML Requirements
\*-------------------------------------*/

html {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

*,
*::before,
*::after {
  -webkit-box-sizing: inherit;
  box-sizing: inherit;
}

/*-------------------------------------*
  #Grid Content
\*-------------------------------------*/

.grid__content {
  padding-right: ($g-gutter / 2);
  padding-left: ($g-gutter / 2);
}

/*-------------------------------------*
  #Grid Container
\*-------------------------------------*/

.grid__container {
  margin-left: auto;
  margin-right: auto;
  width: $container;
  max-width: 100%;
  position: relative;
}

.grid__container--fw {
  margin-left: auto;
  margin-right: auto;
  width: 100%;
  max-width: 100%;
  position: relative;
}

/*================ Grid Row Classes ================*/

[class*=grid__row] {
  position: relative;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -ms-flex-flow: row wrap;
  flex-flow: row wrap;
  width: 100%
}

[class*=grid__col] {
  position: relative;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -ms-flex-flow: column nowrap;
  flex-flow: column nowrap;
  width: 100%
}

.grid__row {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex
}

.grid__col {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex
}

@each $size in $g-responsive-breakpoints {
  @include screen-size($size) {
    .grid__row--#{$size} {
      display: -webkit-box;
      display: -ms-flexbox;
      display: flex;
    }
    .grid__col--#{$size} {
      display: -webkit-box;
      display: -ms-flexbox;
      display: flex;
    }
  }
}

/*================ Grid Positioning Classes ================*/

.justify--fs {
  display: -webkit-box;
