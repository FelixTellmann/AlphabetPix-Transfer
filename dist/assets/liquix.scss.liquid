/*-------------------------------------*
  #Grid Items
\*-------------------------------------*/

[class*=grid__item] {
  position: relative;
  min-height: 1px;
}

.grid__item {
  -webkit-box-flex: 1;
  -ms-flex: 1;
  flex: 1
}

@each $size in $g-responsive-breakpoints {
  .grid__item--#{$size} {
    @include screen-size($size) {
      -webkit-box-flex: 1;
      -ms-flex: 1;
      flex: 1;
    }
  }
}

@for $i from 1 through $columns {
  .grid__item--#{$i} {
    -webkit-box-flex: 0;
    -ms-flex: 0 0 percentage($i / $columns);
    flex: 0 0 percentage($i / $columns);
  }
}

@each $size in $g-responsive-breakpoints {
  @include screen-size($size) {
    @for $i from 1 through $columns {
      .grid__item--#{$i}--#{$size} {
        -webkit-box-flex: 0;
        -ms-flex: 0 0 percentage($i / $columns);
        flex: 0 0 percentage($i / $columns);
      }
    }
  }
}

