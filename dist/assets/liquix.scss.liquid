/* Frame sizes 3 - 9*/

@for $i from 3 through 9 {
  [data-frame-size="#{$i}"].frame--size {
    width: calc(9.5vw * #{$i} + 9.5vw);
    height: 25vw;
    
    @include min-size(1200px) {
      width: calc(114px * #{$i} + 114px);
      height: 300px;
    }
  }
  
  [data-frame-size="#{$i}"].final-artwork__size {
    width: calc(9.5vw * #{$i} + 9.5vw);
    height: 25vw;
    
    @include min-size(1200px) {
      width: calc(85.5px * #{$i} + 85.5px);
      height: 225px;
    }
  }
  
  .create__artwork-to-image {
    [data-frame-size="#{$i}"].frame--size {
      width: calc(228px * #{$i} + 228px);
      height: 600px;
    }
  }
  
  [data-frame-size="#{$i}"] {
    .frame__image--mat {
      left: calc((100% / (#{$i} + 1)) / 2);
      right: calc((100% / (#{$i} + 1)) / 2);
      -webkit-transition: 0.2s all;
      transition: 0.2s all;
      -webkit-transition-timing-function: ease-in-out;
      transition-timing-function: ease-in-out;
      
    }
    .frame__letter-wrapper {
      left: calc((100% / (#{$i} + 1)) / 2);
      width: calc((100% / (#{$i} + 1)) * #{$i});
      -webkit-transition: 0.2s all;
      transition: 0.2s all;
      -webkit-transition-timing-function: ease-in-out;
      transition-timing-function: ease-in-out;
    }
    
    .frame__letter {
      max-width: calc(100% / #{$i});
      min-width: calc(100% / #{$i});
      padding: 0 calc(100% / #{$i} / 57);
      -webkit-transition: 0.2s all;
      transition: 0.2s all;
      -webkit-transition-timing-function: ease-in-out;
      transition-timing-function: ease-in-out;
    }
    input[type='text'].frame__input {
      padding: calc((100% / (#{$i} + 1)) / 1.8095) calc((100% / (#{$i} + 1)) / 1.9);
      -webkit-transition: 0.2s all;
      transition: 0.2s all;
      -webkit-transition-timing-function: ease-in-out;
      transition-timing-function: ease-in-out;
    }
  }
  
}

