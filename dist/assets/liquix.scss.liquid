    font-size: rem(24);
    margin-bottom: 0;
  }
}

.cart-drawer__subtotal-price {
  display: table-cell;
  width: 50%;
  text-align: right;
  font-weight: $font-weight-bold;
  vertical-align: middle;
  letter-spacing: 1px;
}

.cart-drawer__disclaimer {
  margin-bottom: $gutter;
  font-size: rem(14);
  text-align: center;

  @include media-query($medium-up) {
    margin-bottom: $gutter * 2;
  }
}

.cart-drawer__checkout {
